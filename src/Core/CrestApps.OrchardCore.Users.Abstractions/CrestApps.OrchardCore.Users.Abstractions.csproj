<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>$(MSBuildProjectName.Replace(" ", "_").Replace("Abstractions", ""))</RootNamespace>
    <Title>CrestApps OrchardCore Users Abstractions</Title>
    <Description>
      $(OCCMSDescription)

      Abstractions Users module.
    </Description>
    <PackageTags>$(PackageTags) CrestApps OrchardCoreCMS Users Abstractions</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="C:\Code\CrestApps\CrestApps.OrchardCore\src\/CrestAppsLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="OrchardCore.ContentManagement.Abstractions" />
    <PackageReference Include="OrchardCore.Users.Abstractions" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Common\CrestApps.Support\CrestApps.Support.csproj" />
  </ItemGroup>

</Project>
