    <PackageReference Include="OrchardCore.Users" />
    <PackageReference Include="OrchardCore.ReCaptcha.Core" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Core\CrestApps.OrchardCore.Abstractions\CrestApps.OrchardCore.Abstractions.csproj" />
