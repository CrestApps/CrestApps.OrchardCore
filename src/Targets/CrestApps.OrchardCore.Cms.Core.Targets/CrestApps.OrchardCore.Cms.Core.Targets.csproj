<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!-- NuGet properties-->
    <Title>Target for Modular OrchardCore CMS Application</Title>
    <Description>
      $(CrestAppsDescription)

      Build-time targets for CrestApps OrchardCore CMS applications.
      This package provides build-time wiring via .props/.targets files.
      For runtime module dependencies, use CrestApps.OrchardCore.Application.Cms.Core instead.
    </Description>
    <PackageTags>$(PackageTags) OrchardCoreCMS CMS</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

</Project>
